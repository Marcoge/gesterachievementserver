<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="com.gester.GesterAchievementServer.LoggedAchievement" table="LOGGEDACHIEVEMENT">  
    <id name="iD" column="id">  
     <generator class="native"></generator>  
    </id>
	<property name="userEmail" column="userEmail"></property>
            
    <property name="achievementID" column="achievement_id"></property>  
    <property name="comment" column="comment"></property>  
    <property name="pic" column="pic"></property>
    <property name="timeStamp" column="time_stamp"></property>
    <property name="priv" column="private"></property>
            
  </class>  
            
 </hibernate-mapping>  